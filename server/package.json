{
  "name": "mob-rest-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "server/node_modules/.bin/eslint . --ext .ts",
    "compile": "server/node_modules/.bin/tsc",
    "prebuild": "npm run lint",
    "build": "npm run clean && npm run lint && npm run compile && npm run copy-assets",
    "prestart": "npm run lint && npm run compile && npm run copy-assets",
    "start": "set DEBUG=express:* & node dist/index.js",
    "start:dev": "server/node_modules/.bin/nodemon --config nodemon.json --exec npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-assets": "server/node_modules/.bin/copyfiles -u 1 resources/** dist/resources",
    "clean": "server/node_modules/.bin/rimraf dist/"
  },
  "author": "Tomás El Fakih; Robert González",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.2",
    "socket.io-client": "^4.4.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/grunt": "^0.4.27",
    "@types/node": "^16.11.17",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.5.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  }
